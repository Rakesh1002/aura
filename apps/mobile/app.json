{
  "expo": {
    "name": "Aura",
    "slug": "aura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aura",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rakeshroushan1002.aura"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0d0015",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rakeshroushan1002.aura"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0d0015",
          "dark": {
            "backgroundColor": "#0d0015"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8751f9ec-c18f-48d5-9277-1217c0f3ca39"
      }
    },
    "owner": "rakeshroushan1002",
    "updates": {
      "url": "https://u.expo.dev/8751f9ec-c18f-48d5-9277-1217c0f3ca39"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}